#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int age,wt;
    float hg;
    scanf("%d%d%f",&age,&wt,&hg);
    if((age>=18)&&(wt>=45)&&(hg>=5.5))
    {
        printf("He or She can donate blood");
    }
    else
    {
        printf("He or She unable to donate blood");
    }
    return 0;
}
