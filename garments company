#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n],i,j,cnt=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
    for(j=1;j<pow(10,6);j++)
    {
        if(j*j==a[i])
            cnt++;
    }
    }
    printf("%d",cnt);
    return 0;
}
}
